{
  "name": "compass-3d-courses-web",
  "version": "0.1.0",
  "private": true,
  "description": "Проект представляет собой веб-сайт, целью которого является предоставление обучающих материалов и курсов по программе \"Компас 3D\".",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NotACat1/COMPASS-3D-courses-WEB"
  },
  "keywords": [
    "web-course",
    "compass-3d",
    "education",
    "web-development",
    "React",
    "Redux",
    "SCSS",
    "react-markdown",
    "ESLint",
    "Prettier",
    "Stylelint",
    "HTMLHint",
    "husky",
    "Normalize.css"
  ],
  "author": "NotACat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotACat1/COMPASS-3D-courses-WEB/issues"
  },
  "homepage": "https://github.com/NotACat1/COMPASS-3D-courses-WEB#readme",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less}": [
      "stylelint --fix"
    ],
    "*.{json,ya?ml,md}": [
      "prettier --write"
    ],
    "*.html": [
      "htmlhint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-responsive": "^9.0.2",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.69.7",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d dist",
    "husky-inst": "npx husky install && chmod +x .husky/pre-commit .husky/commit-msg",
    "msg-commit": "npx commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "npx lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "react-router-dom": "^6.21.2",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-less": "^3.0.1",
    "stylelint-scss": "^6.0.0"
  }
}
